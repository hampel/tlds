{
    "name": "hampel/tlds",
	"homepage" : "https://bitbucket.org/hampel/tlds",
    "description": "Fetches the latest list of Top Level Domains from IANA, plus Laravel classes for validating TLDs",
	"authors": [{
		"name": "Simon Hampel",
		"email": "simon@hampelgroup.com"
	}],
	"support": {
		"issues": "https://bitbucket.org/hampel/tlds/issues",
		"source": "https://bitbucket.org/hampel/tlds/src"
	},
	"keywords": [
		"tld",
		"domain",
		"iana",
		"domain name",
		"laravel"
	],
	"license" : [
		"MIT"
	],
    "require": {
		"illuminate/support": "^8.0|^9.0|^10.0",
		"guzzlehttp/guzzle": "^6.0|^7.0",
		"hampel/validate": "~2.2",
        "php": ">=5.5"
    },
	"require-dev": {
		"illuminate/container": "^8.0|^9.0|^10.0",
		"illuminate/validation": "^8.0|^9.0|^10.0"
		"phpunit/phpunit": "^10.0",
		"mockery/mockery": "~1.0"
	},
    "suggest": {
		"hampel/validate-laravel": "Additional validation service rules"
    },
	"conflicts": {
		"hampel/validate-laravel": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "Hampel\\Tlds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hampel\\Tlds\\": "tests/"
        }
	},
 	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"laravel": {
			"providers": [
				"Hampel\\Tlds\\TldServiceProvider"
			],
			"aliases": {
				"Tlds": "Hampel\\Tlds\\Facades\\Tlds"
			}
		}
	}
}
